#VRML V2.0 utf8

Background {
   
groundColor       [0.01 0.01 1]
skyColor [1 0.1 0.1, 0.2 1 0.2, 0.3 0.3 1]
skyAngle [0.78, 1.57]

}





DEF PEPA Transform{ 

children [




Shape {				
         	 appearance Appearance { 
    
             		material Material  { 
            			 diffuseColor 0 0 0
             			       	 }
						}
      	 geometry Box{
       		 size 6.0 3.0 0.3
   		             }

	}

		Transform{
			translation 0.0 0.0 0.3
				
			children[
				Shape { 
         				
  appearance Appearance {
   material Material { diffuseColor 1 1 1 }
#TextureTransform {
 #  center .50 .50 }
   texture ImageTexture { url "G:\CPS\int-h-m\telefono\TECLAS2.jpg" }
  }
  geometry IndexedFaceSet {
   coord Coordinate {
    point [ -3 -1.5 0, 3 -1.5 0, 3 1.5 0, -3 1.5 0 ]
   }
   coordIndex [0,1,2,3,-1]
   texCoord TextureCoordinate {
    point [0 0, 1 0, 1 1, 0 1]
   }
   texCoordIndex [0,1,2,3,-1]
  }


					}
				  ]
			    }

		


Transform {
#translation 0 2.5 0
    translation 0 2.5 0.2
	  rotation 1 0 0 0.2
    children [
        		Shape {
            	appearance Appearance {
               		 material Material {
					diffuseColor 0 0 0		
							 }
           			       }
            	geometry Box {
               		 size 6.0 1.5 0.3 
           		     }
        			}

Transform{
			translation 0.0 0.0 0.3
				
			children[
				Shape { 
         				
  appearance Appearance {
   material Material { diffuseColor 1 1 1 }
   texture ImageTexture { url "G:\CPS\int-h-m\telefono\display.jpg" }
 				 }
  geometry IndexedFaceSet {
   coord Coordinate {
    point [ -3 -0.7 0, 3 -0.7 0, 3 0.7 0, -3 0.7 0 ]
   				   }
   coordIndex [0,1,2,3,-1]
   texCoord TextureCoordinate {
    point [0 0, 1 0, 1 1, 0 1]
   				}
   texCoordIndex [0,1,2,3,-1]
  }
}
]
}



    	     ]
		}

Transform {
    rotation 0 0 1 1.57
	  translation 0 1.65  0
    children [
        		Shape {
            	appearance Appearance {
               		 material Material {
					diffuseColor 0.09 0.09 0.09
							 }
           			       }
            	geometry Cylinder {
               		 radius 0.2
                		 height 6.0
           		     }
        			}
    	     ]
		}


]
}
DEF TIMER TimeSensor { 

loop TRUE 

cycleInterval 15 

startTime 1 #comienzo automático 

stopTime 0 #comienzo automático 

} 

#la esfera rota sobre sí misma 

DEF SPHERE_ROTATE OrientationInterpolator { 

key [ 0, 0.5, 1 ] 

keyValue [ 0 1 0 0, 0 1 0 3.14, 0 1 0 6.28 ] 

}

#Primero la ruta del timer en laPpositionInterpolator 

ROUTE TIMER.fraction_changed TO SPHERE_ROTATE.set_fraction 

#luego el interpolator en el transform 

ROUTE SPHERE_ROTATE.value_changed TO PEPA.set_rotation 














